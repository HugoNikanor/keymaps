xkb_symbols "dvorak_a6" {
	include "se(basic)"

	name[Group1]="Swedish (Dvorak A6)";

	key <LWIN> { [ Super_L                                           ] };
	key <RWIN> { [ Super_R                                           ] };
	//key <LFSH> { [ ISO_Level3_Shift	] };
	//key <LCTL> { [ Shift_L		] };
	//key <ESC>  { [ Caps_Lock ] };
	//key <CAPS> { [ Escape ] };

    key <SPCE>{ [ space,       space,      space,               space] };

	//key <RTSH> { [ ISO_Level3_Shift	] };
